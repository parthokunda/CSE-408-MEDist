<body>
  <!-- main div -->
  <div class="box">
    <div class="group">
      <div class="overlap">
        <div class="rectangle"></div>
        <div class="text-wrapper">Meet After <%= meetAfter %> Days</div>
        <div class="med-header">
          <div class="navbar">
            <div class="div">Name</div>
            <div class="text-wrapper-2">Dosage</div>
            <div class="text-wrapper-3">When</div>
            <div class="text-wrapper-4">Duration</div>
          </div>

          <hr style="margin-top: 25px" />
        </div>

        <div class="tests">
          <div class="advice-header">Tests</div>
          <% if(test && test.length> 0) { test.forEach((entry) => { %>
            <div class="advice-entry">
              <%= entry %>
            </div>
            <% })} %>
        </div>

        <div class="advices">
          <div class="advice-header">Advice</div>
          <% advices.forEach((advice)=> { %>
            <div class="advice-entry">
              <%= advice %>
            </div>
            <% }) %>
        </div>

        <div class="medicines">
          <div class="med-list">
            <% if(Medicines.length> 0) { Medicines.forEach((brandInfo) => {
              %>

              <div class="med-entry">
                <div class="medBefAft">
                  <%= brandInfo.when %>
                </div>
                <div class="medDuration">
                  <%= brandInfo.duration %>
                </div>
                <div class="medName">
                  <img class="medImage" src=<%=brandInfo.DosageForm.img_url %>
                  />
                  <%= brandInfo.Brand.name %>
                    <%= brandInfo.Brand.strength %>
                </div>
                <div class="medWhen">
                  <%= brandInfo.dosage %>
                </div>
              </div>

              <% }) } %>
          </div>

          <div class="prescription-header">
            <div class="overlap-group">
              <div class="doctor-info">
                <div class="doctorName">
                  Dr. <%= Header.DoctorPortionInfo.DoctorInfo.name %>
                </div>
                <div class="doctor-additional">
                  <div class="contact">
                    Contact: <%= Header.DoctorPortionInfo.DoctorInfo.phone %>
                  </div>
                  <div class="degree">
                    <%= Header.DoctorPortionInfo.DoctorInfo.degrees.join(", ") %>
                      </div>
                      <div class=" department">
                      <%= Header.DoctorPortionInfo.Specialization.name %>
                  </div>
                </div>
              </div>
              <div class="prescription-info">
                <div class="prescriptionDate">
                  Date: <%= new Date(Header.AppointmentPortionInfo.time).toLocaleDateString() %>
                </div>
              </div>
            </div>
          </div>
          <img class="rxImg" src="https://www.freeiconspng.com/thumbs/rx-icon/rx-icon-6.png" />
        </div>

        <div class="leftBar">
          <div class="history">
            <div class="leftBarHeadings">Past History</div>
            <% if(past_history && past_history.length> 0) { past_history.forEach((history) => { %>
              <div class="PastHistoryEntry">
                <%= history %>
              </div>
              <% })} %>
          </div>
          <div class="Diagnosis">
            <div class="leftBarHeadings">Diagnosis:</div>
            <% if(diagnosis && diagnosis.length> 0) { diagnosis.forEach((entry) => { %>
              <div class="leftBarEntries">
                <%= entry %>
              </div>
              <% })} %>

          </div>
          <div class="symptom">
            <div class="leftBarHeadings">Symptoms:</div>
            <% if(symptoms && symptoms.length> 0) { symptoms.forEach((entry) => { %>
              <div class="symptomEntry">
                <%= entry %>
              </div>
              <% })} %>
          </div>
        </div>

        <div class="presc-patient-info">
          <div class="patientDiv">
            <div class="age">Age: <%= Header.PatientPortionInfo.age %>
            </div>
            <div class="height">
              Height: <%= Header.PatientPortionInfo.height.feet %>’ <%= Header.PatientPortionInfo.height.inches %>’’
            </div>
            <div class="weight">
              Weight: <%= Header.PatientPortionInfo.weight %> kgs
            </div>
            <div class="name">
              Name: <%= Header.PatientPortionInfo.name %>
            </div>
            <div class="blood">
              Blood: <%= Header.PatientPortionInfo.bloodGroup %>
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
  <!-- end of main div -->
</body>


